{
	"dirsEnsure": [
		".vscode",
		"config",
		"lib",
		"src"
	],
	"files": {
		"package.json": {
			"type": "handle-json",
			"isPackage": true,
			"handles": [
				"concat-key-value|scripts",
				"concat-key-value|dependencies",
				"concat-key-value|devDependencies",
				"concat-array|typesSource"
			]
		},
		"LICENSE": "copy|base",
		".vscode/extensions.json": {
			"type": "handle-json",
			"handles": [
				"concat-array|recommendations"
			]
		},
		".vscode/launch.json": {
			"type": "handle-json",
			"handles": [
				"concat-array|configurations|name"
			]
		},
		".vscode/settings.json": {
			"type": "handle-json",
			"handles": [
				"concat-key-value"
			]
		},
		".gitignore|gitignore.txt": "concat-text-line",
		"jsconfig.json": {
			"type": "handle-json",
			"handles": [
				"concat-array|exclude"
			]
		},
		"eslint.config.js": "copy|base",
		".npmrc|npmrc.txt": "copy|base"
	}
}
